	keywords = {Gaussian_processes Laplace_approximation computation statistics},
	number = 2,
	pages = {319--392},
	publisher = {Blackwell Publishing Ltd},
	timestamp = {2012-08-10T01:49:52.000+0200},
	title = {Approximate {Bayesian} inference for latent {Gaussian} models by using integrated nested {Laplace} approximations},
